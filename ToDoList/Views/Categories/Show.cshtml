<h3>Here are all the items in this category:</h3>

<ol>
@foreach (var item in @Model["items"])
{
  <li><a href='/categories/@Model["category"].GetId()/items/@item.GetId()'>@item.GetDescription()</a>, @item.GetDueDate()</li>
}
</ol>
<h2><a href= "/categories/@Model["category"].GetId()/items/sort"> Sort items </a></h2>
<p><a href='/categories/@Model["category"].GetId()/items/new'>Add a new item</a></p>
<p><a href='/categories'>Return to category list</a></p>
<p><a href='/'>Return to Main Page</a></p>
